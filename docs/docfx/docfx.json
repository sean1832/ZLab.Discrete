{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src": [
        {
          "src": "../../ZLab.Discrete",
          "files": [
            "**/*.csproj"
          ]
        }
      ],
      "dest": "api",
      "memberLayout": "separatePages"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "templates/singulinkfx"
    ],
    "globalMetadata": {
      "_appName": "ZLab.Discrete",
      "_appTitle": "ZLab.Discrete",
      "_appFooter": "<strong>Developed by <a href='https://github.com/sean1832' style='color: lightblue !important;'>Zeke Zhang</a></strong>",
      "_enableSearch": true,
      "_appLogoPath": "images/icon_dark_trans.png"
    }
  }
}